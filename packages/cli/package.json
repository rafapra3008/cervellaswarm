{
  "name": "cervellaswarm",
  "version": "0.1.0",
  "description": "16 AI agents working as a team for your project. Not an assistant - a TEAM.",
  "main": "src/index.js",
  "bin": {
    "cervellaswarm": "./bin/cervellaswarm.js",
    "cs": "./bin/cervellaswarm.js"
  },
  "type": "module",
  "scripts": {
    "start": "node bin/cervellaswarm.js",
    "test": "node --test test/commands/*.test.js test/agents/*.test.js test/edge-cases.test.js",
    "test:watch": "node --test --watch",
    "test:coverage": "node --test --experimental-test-coverage",
    "test:commands": "node --test test/commands/",
    "test:agents": "node --test test/agents/",
    "test:session": "node --test test/session/",
    "test:edge": "node --test test/edge-cases.test.js",
    "test:integration": "node --test test/integration/",
    "hardtests": "./test/run_hardtests.sh",
    "lint": "eslint src/"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "claude",
    "coding",
    "assistant",
    "team",
    "development",
    "cli"
  ],
  "author": "Rafa & Cervella",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cervellaswarm/cli"
  },
  "homepage": "https://cervellaswarm.dev",
  "bugs": {
    "url": "https://github.com/cervellaswarm/cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin/",
    "src/",
    "LICENSE",
    "README.md",
    ".env.example"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@inquirer/prompts": "^7.2.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "figures": "^6.1.0",
    "handlebars": "^4.7.8",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@inquirer/testing": "^3.0.4",
    "eslint": "^9.17.0"
  }
}
